    <entry>
      <name>populate Concept table</name>
      <description/>
      <type>SQL</type>
      <sql>insert into Concept (CODE, CODESYSTEM_ID, active)
SELECT 
	distinct c.id, 9, 1
FROM
	careconnect.tempConcept c
	where active = 1 and id not in (select code from careconnect.Concept where CODESYSTEM_ID=9); </sql>
