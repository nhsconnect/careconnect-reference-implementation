      <name>Populate Temp tables</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE if NOT EXISTS `tempConcept` (
  `idConcept` int(11) NOT NULL AUTO_INCREMENT,
  `id` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `effectiveTime` date DEFAULT NULL,
  `active` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `moduleId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `definitionStatusId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  PRIMARY KEY (`idConcept`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

CREATE TABLE IF NOT EXISTS `tempDescription` (
  `idDescription` int(11) NOT NULL AUTO_INCREMENT,
  `id` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `effectiveTime` date DEFAULT NULL,
  `active` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `moduleId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `conceptId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `languageCode` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `typeId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `term` varchar(1024) COLLATE utf8_bin DEFAULT NULL,
  `caseSignificanceId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `CONCEPT_DESIGNATION_ID` bigint(20),
  `designationUse` int(11),
  `CONCEPT_ID` bigint(20),
  PRIMARY KEY (`idDescription`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

CREATE TABLE if NOT EXISTS careconnect.`tempRelationship` (
  `idRelationship` int(11) NOT NULL AUTO_INCREMENT,
  `id` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `effectiveTime` date DEFAULT NULL,
  `active` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `moduleId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `sourceId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `destinationId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `relationshipGroup` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `typeId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `characteristicTypeId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `modifierId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  PRIMARY KEY (`idRelationship`),
	KEY `ID_idConcept` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


CREATE TABLE if NOT EXISTS careconnect.`tempSimple` (
  `idSimple` int(11) NOT NULL AUTO_INCREMENT,
  `id` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `effectiveTime` date DEFAULT NULL,
  `active` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `moduleId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `refsetId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  `referencedComponentId` varchar(45) COLLATE utf8_bin DEFAULT NULL,
  PRIMARY KEY (`idSimple`),
  KEY `ID_idSimple` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=671910 DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

truncate table careconnect.tempSimple;

truncate table `tempDescription`;

truncate table `tempConcept`;

truncate table `tempRelationship`;</sql>
