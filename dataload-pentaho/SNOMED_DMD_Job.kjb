      <name>Update Concept display term</name>
      <description/>
      <type>SQL</type>
      <sql>update Concept c 
join careconnect.ConceptDesignation d on c.CONCEPT_ID = d.CONCEPT_ID
 set c.DISPLAY = d.term
where designationUse=0;</sql>
