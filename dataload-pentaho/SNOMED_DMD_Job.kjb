    <entry>
      <name>populate Concept Designation</name>
      <description/>
      <type>SQL</type>
      <sql>SET SQL_SAFE_UPDATES = 0;

update
careconnect.tempDescription d
join careconnect.Concept c on d.conceptId = c.CODE 
set d.CONCEPT_ID = c.CONCEPT_ID
where  c.CODESYSTEM_ID = 9 ;


update
careconnect.tempDescription d
join careconnect.ConceptDesignation c on d.id = c.designationId 
set d.CONCEPT_DESIGNATION_ID = c.CONCEPT_DESIGNATION_ID;

insert into careconnect.ConceptDesignation(designationId,term,CONCEPT_ID,designationUse,active)
SELECT 
	distinct d.id,d.term,d.CONCEPT_ID,
	case d.typeId 
	   when '900000000000003001' then 0
	   when '900000000000013009' then 1
	   when '900000000000550004' then 2
	end 'type'
	,1 	
FROM
	careconnect.tempDescription d
where d.active = 1 and CONCEPT_DESIGNATION_ID is null; </sql>
